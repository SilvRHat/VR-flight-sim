}
function init()
    for lvar, default in pairs(lvars) do
        varset(&apos;L:&apos;..lvar, default)
    end
    for i=0, 15, 1 do
        varset(&apos;L:_toggle&apos;..i, 0)
    end
    varset(&apos;L:_toggle0&apos;, 1)
end

init()
